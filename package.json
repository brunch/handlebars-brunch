{
  "name": "handlebars-brunch",
  "version": "1.3.0",
  "description": "Adds Handlebars.js support to brunch.",
  "author": "Paul Miller (http://paulmillr.com/)",
  "homepage": "https://github.com/brunch/handlebars-brunch",
  "repository": {
    "type": "git",
    "url": "git@github.com:brunch/handlebars-brunch.git"
  },
  "main": "./lib/index",
  "scripts": {
    "prepublish": "coffee -o lib/ src/",
    "postinstall": "node setup.js postinstall",
    "test": "node setup.js test"
  },
  "engines": {
    "node": "~0.6.10 || 0.8 || 0.9"
  },
  "dependencies": {
    "coffee-script": "~1.3.3",
    "handlebars": "~1.0.7",
    "mkdirp": "~0.3.4"
  },
  "devDependencies": {
    "mocha": "1.2.2",
    "chai": "1.1.0"
  },
  "config": {
    "dist": "lib/handlebars.runtime.js",
    "deps": "base utils runtime",
    "depsDir": "node_modules/handlebars/lib/handlebars",
    "removeExports": "^// BEGIN\\(BROWSER\\).*([\\s\\S]*)^// END\\(BROWSER\\)"
  }
}
